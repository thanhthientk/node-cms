<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="keywords" content="{{ Settings.keywords }}">

    {#SEO#}
    {% include './partial/seo.xoo' %}

    <title>{{ pageTitle + ' - ' if pageTitle }} Gynos</title>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/libs/font-awesome.min.css"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/libs/bootstrap.css"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/libs/owl.carousel.css"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/libs/owl.transitions.css"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/libs/owl.theme.css"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/color.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/sweetalert/sweetalert.css">
    {% block css %}
    {% endblock %}
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/theme.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/responsive.css" media="all"/>
    <link rel="stylesheet" type="text/css" href="/gynos/assets/css/browser.css" media="all"/>
</head>
<body>
<div class="wrap">

    {% include './partial/header.xoo' %}
    <!-- End Header -->

    {% block content %}
    {% endblock %}

    <!-- End Content -->
    {% include './partial/footer.xoo' %}
    <!-- End Footer -->
    <a href="#" class="scroll-top radius"><i class="fa fa-angle-up" aria-hidden="true"></i></a>

    {#Spiner#}
    <div class="spinner hidden">
        <div class="rect1"></div>
        <div class="rect2"></div>
        <div class="rect3"></div>
        <div class="rect4"></div>
        <div class="rect5"></div>
    </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="/gynos/assets/js/libs/bootstrap.min.js"></script>
<script type="text/javascript" src="/gynos/assets/js/libs/owl.carousel.js"></script>
<script type="text/javascript" src="/gynos/assets/js/libs/jquery.jcarousellite.js"></script>
<script type="text/javascript" src="/gynos/assets/js/libs/jquery.elevatezoom.js"></script>
<script type="text/javascript" src="/hotel/assets/sweetalert/sweetalert.min.js"></script>
{% block js %}
{% endblock %}
<script type="text/javascript" src="/gynos/assets/js/theme.js"></script>
</body>
</html>